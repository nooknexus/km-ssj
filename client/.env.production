HEALTH_CLIENT_ID=<YOUR_PRODUCTION_CLIENT_ID>
HEALTH_REDIRECT_URI=https://km.plkhealth.go.th/api/auth/healthid

# ===== Provider ID (Production) =====
PROVIDER_CLIENT_ID=<YOUR_PRODUCTION_PROVIDER_ID>
